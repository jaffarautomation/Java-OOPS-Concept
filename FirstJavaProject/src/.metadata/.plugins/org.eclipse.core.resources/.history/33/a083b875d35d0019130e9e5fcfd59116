package Lesson2;

public class SwitchStatement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int X=90;
      
     switch (X)
     {
    	 
     case 100 :   
    	 System.out.println("Vodafone");
    	 break ;
     case 60 :
    	 System.out.println("good");
       break;
     case 90 :
    	 System.out.println(" V good");
     }
     
	}

}

package Lesson7Inheritance;

public class Myclass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		VeryAdvancecalculator resultadd = new VeryAdvancecalculator();
		//int result1= resultadd.addition(5, 4);
		int result2= resultadd.subtract(500, 455);
		int result3= resultadd.multiply(5, 45);
		
		resultadd.addition(500, 56);
		
		//System.out.println(result1);
		System.out.println(result2);
		System.out.println(result3);
	}

	
	// here VeryAdvancecalculator extend the AdvanceCalculator class and again it
	//extend the calculator class.
	// so all the methods and variable are inherit by the VeryAdvancecalculator.
	// so this concept is called multilevel inheritance.
	
	
}

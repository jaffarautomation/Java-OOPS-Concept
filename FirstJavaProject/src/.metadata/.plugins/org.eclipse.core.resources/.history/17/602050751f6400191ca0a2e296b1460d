package lesson5Static;



public class Emp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		employee navin=new employee();
		navin.eid=123;
		navin.salary=4000;
		navin.ceo="Mahesh";
		navin.show();
		employee rahul = new employee();
		rahul.eid=1234;
		rahul.salary=5000;
		rahul.ceo="Mahesh";
		
		
		rahul.show();
		
				

	}

}

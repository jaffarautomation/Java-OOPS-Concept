package Lesson3;

public class StudentClass {
	
	int id;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String n) {
		this.name = n;
	}
	int age;
	String name;

	
	
	
}
